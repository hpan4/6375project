    /**
     * Calculate result to answer.
     * 
     */
    public void calculate(){
    	answer=0;
	    try {
	      //calculate 2nd greatest divisor/factor
	      if (question!=0){
	    	  for (int i=1; i<=question/2; i++){
	    		  if (question%i==0)
	    			  answer=i;
	    	  } //end for loop
	      } //end if
	    } catch (Exception e) {
	      System.err.println(e.getClass().getName()+": "+e.getMessage());
	      System.exit(0);
	    }
    } //end calculate()
